# Clock
NET "top_clk" LOC="B8";
# Define a new timing constraint indicating a 25 MHz clock period
NET "top_clk" TNM_NET = "top_clk";
TIMESPEC "TS_clk" = PERIOD "top_clk" 20 ns HIGH 50 %;

