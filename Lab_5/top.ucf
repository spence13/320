NET "sw<5>" LOC=L13;
NET "sw<6>" LOC=N17;
NET "sw<7>" LOC=R17;

#attach buttons
NET "btn<0>" LOC=B18;
NET "btn<1>" LOC=D18;
NET "btn<2>" LOC=E18;
